<view class="cateList"><block wx:if="{{cateObj.category}}"><swiper class="swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{item}}" mode></image></view></swiper-item></block></swiper></block><view class="frontDesc">{{''+cateObj.category.frontDesc+''}}</view><view class="frontName">{{''+cateObj.category.frontName+''}}</view><block wx:if="{{cateObj.itemList}}"><view class="goodsList"><block wx:for="{{cateObj.itemList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['cateObj.itemList','id',item.id]]]]]]]}}" class="goodsItem" bindtap="__e"><image src="{{item.primaryPicUrl}}" mode></image><view class="goodsDesc">{{''+item.name+''}}</view><view class="goodsPrice">{{'￥'+item.retailPrice+'/斤'}}</view></view></block></view></block></view>